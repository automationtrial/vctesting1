  <JOBI>
    <OH>
      <row>
        <OH_IDNR>1008055</OH_IDNR>
        <OH_AEVERSION>11.2</OH_AEVERSION>
        <OH_CHILDFLAGS>00000000000000000000000000000000</OH_CHILDFLAGS>
        <OH_CRDATE>2018-06-12 08:30:01.0</OH_CRDATE>
        <OH_CRUSERIDNR>1001003</OH_CRUSERIDNR>
        <OH_LASTRUNS>AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA</OH_LASTRUNS>
        <OH_MODUSERIDNR>1001003</OH_MODUSERIDNR>
        <OH_NAME>PCK.ITPA_SHARED.PRV.INCLUDE.SET_BACKUP_DIRECTORY</OH_NAME>
        <OH_OTYPE>JOBI</OH_OTYPE>
        <OH_VERSIONINGID> </OH_VERSIONINGID>
      </row>
    </OH>
    <OT>
      <OT_Type value="0">
        <OT_CONTENT>! This Include is used in Process tab of Workflows, so that &amp;UC4RB_AGENT_OS# is not available.</OT_CONTENT>
        <OT_CONTENT>! Path to report will be normalized where it is used.</OT_CONTENT>
        <OT_CONTENT>! GET RUNID OF WORKFLOW</OT_CONTENT>
        <OT_CONTENT>:SET &amp;RUNID# = SYS_ACT_RESTART_ME_NR()</OT_CONTENT>
        <OT_CONTENT>:SET &amp;RUNID# = FORMAT("&amp;RUNID#", "")</OT_CONTENT>
        <OT_CONTENT>:SET &amp;UC4RB_BACKUP_DIRECTORY# = GET_VAR('UC_EX_PATH_BACKUP', &amp;AGENT#)</OT_CONTENT>
        <OT_CONTENT>:SET &amp;UC4RB_CONVERTED_STRING# = "&amp;UC4RB_BACKUP_DIRECTORY#/&amp;$CLIENT#-&amp;LOGIN#/&amp;$PHYS_DATE_YYYYMMDD#/&amp;RUNID#"</OT_CONTENT>
        <OT_CONTENT>:SET &amp;UC4RB_BACKUP_DIRECTORY# = &amp;UC4RB_CONVERTED_STRING#</OT_CONTENT>
        <OT_CONTENT>:SET &amp;UC4RB_BACKUP_REPORT# = "&amp;UC4RB_BACKUP_DIRECTORY#/report.txt"</OT_CONTENT>
        <OT_CONTENT>:IF &amp;$RUNMODE# EQ "NORMAL"</OT_CONTENT>
        <OT_CONTENT>: PUB &amp;UC4RB_BACKUP_DIRECTORY#, , "TASK"</OT_CONTENT>
        <OT_CONTENT>: PUB &amp;UC4RB_BACKUP_REPORT#, , "TASK"</OT_CONTENT>
        <OT_CONTENT>:ENDIF</OT_CONTENT>
        <OT_CONTENT></OT_CONTENT>
      </OT_Type>
    </OT>
  </JOBI>
