        <OT_CONTENT></OT_CONTENT>
        <OT_CONTENT>!find % character and add escape character in URIs</OT_CONTENT>
        <OT_CONTENT>:SET &amp;POST# = STR_FIND(&amp;UC4RB_DB_SCRIPT_URI#, "%")</OT_CONTENT>
        <OT_CONTENT>:IF &amp;POST# &lt;&gt; 0</OT_CONTENT>
        <OT_CONTENT>:  SET &amp;UC4RB_DB_SCRIPT_URI# = STR_SUBSTITUTE(&amp;UC4RB_DB_SCRIPT_URI#, "%", "%%")</OT_CONTENT>
        <OT_CONTENT>:ENDIF</OT_CONTENT>
        <OT_CONTENT></OT_CONTENT>
        <OT_CONTENT>:SET &amp;POST# = STR_FIND(&amp;UC4RB_DB_DRIVER_JAR#, "%")</OT_CONTENT>
        <OT_CONTENT>:IF &amp;POST# &lt;&gt; 0</OT_CONTENT>
        <OT_CONTENT>:  SET &amp;UC4RB_DB_DRIVER_JAR# = STR_SUBSTITUTE(&amp;UC4RB_DB_DRIVER_JAR#, "%", "%%")</OT_CONTENT>
        <OT_CONTENT>:ENDIF</OT_CONTENT>
        <OT_CONTENT></OT_CONTENT>
        <OT_CONTENT>:SET &amp;POST# = STR_FIND(&amp;UC4RB_DB_TNSNAMES_ORA#, "%")</OT_CONTENT>
        <OT_CONTENT>:IF &amp;POST# &lt;&gt; 0</OT_CONTENT>
        <OT_CONTENT>:  SET &amp;UC4RB_DB_TNSNAMES_ORA# = STR_SUBSTITUTE(&amp;UC4RB_DB_TNSNAMES_ORA#, "%", "%%")</OT_CONTENT>
        <OT_CONTENT>:ENDIF</OT_CONTENT>
        <OT_CONTENT></OT_CONTENT>
        <OT_CONTENT></OT_CONTENT>
        <OT_CONTENT>!executes a given sql script file</OT_CONTENT>
        <OT_CONTENT>:INC PCK.ITPA_SHARED.PRV.INCLUDE.LOGGING.HEADER.EXECUTION</OT_CONTENT>
        <OT_CONTENT></OT_CONTENT>
        <OT_CONTENT>:INCLUDE PCK.AUTOMIC_JDBC.PRV.INCLUDE.EXECUTE_SQL_STATEMENT</OT_CONTENT>
        <OT_CONTENT></OT_CONTENT>
        <OT_CONTENT>:IF &amp;UC4RB_DB_SCRIPT_ORDER# &lt;&gt; ""</OT_CONTENT>
        <OT_CONTENT>:  SET &amp;UC4RB_SQL_COMMAND# = STR_CAT(&amp;UC4RB_SQL_COMMAND#, ' -exe_order "&amp;UC4RB_DB_SCRIPT_ORDER#"')</OT_CONTENT>
        <OT_CONTENT>:ENDIF</OT_CONTENT>
        <OT_CONTENT></OT_CONTENT>
        <OT_CONTENT>:JCL_CONCAT_CHAR "?"</OT_CONTENT>
        <OT_CONTENT>java -jar "&amp;JDBC_TOOL_JAR#" database SQLFeature -script "&amp;UC4RB_DB_SCRIPT_URI#" ?</OT_CONTENT>
        <OT_CONTENT>&amp;UC4RB_SQL_COMMAND# -sep "&amp;UC4RB_DB_SCRIPT_SEPR#"</OT_CONTENT>
        <OT_CONTENT>:JCL_CONCAT_CHAR</OT_CONTENT>
