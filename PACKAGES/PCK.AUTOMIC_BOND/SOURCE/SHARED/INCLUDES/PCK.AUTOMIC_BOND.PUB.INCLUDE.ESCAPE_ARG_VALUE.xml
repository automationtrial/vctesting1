    <ODOC>
      <Docu type="0">
<![CDATA[It escapes the Nexus input parameters and wrap the parameter surrounded by quotes(").
]]>      </Docu>
    </ODOC>
    <OT>
      <OT_Type value="0">
        <OT_CONTENT>! We escape double-quotes for java Strings</OT_CONTENT>
        <OT_CONTENT>: SET &amp;TEMP# = STR_SUB("TEMP_OPTION_VALUE", """", "\""")</OT_CONTENT>
        <OT_CONTENT></OT_CONTENT>
        <OT_CONTENT>! then we add double-quotes to String to preserve spaces in value</OT_CONTENT>
        <OT_CONTENT>: SET &amp;TEMP# = STR_CAT(""" ", &amp;TEMP#)</OT_CONTENT>
        <OT_CONTENT>: SET &amp;TEMP# = STR_CAT(&amp;TEMP#, " """)</OT_CONTENT>
        <OT_CONTENT></OT_CONTENT>
        <OT_CONTENT>: SET &amp;UC4RB_OPTION_VALUE# = &amp;TEMP#</OT_CONTENT>
      </OT_Type>
    </OT>
  </JOBI>
